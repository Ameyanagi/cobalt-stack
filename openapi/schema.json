{
  "openapi": "3.1.0",
  "info": {
    "title": "Cobalt Stack API",
    "description": "Full-stack application with Rust backend (Axum + SeaORM) and Next.js frontend",
    "contact": {
      "name": "API Support"
    },
    "license": {
      "name": ""
    },
    "version": "0.1.0"
